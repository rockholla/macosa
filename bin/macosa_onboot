#!/bin/bash

macosa_bin_dir="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

for boot_script in $macosa_bin_dir/onboot/*; do
  if [ $boot_script == '.keep' ]; then
    continue
  fi
  osascript -e 'tell application "Terminal" to do script "'"$(cat $boot_script)"'" in front window'
  rm $boot_script
done
